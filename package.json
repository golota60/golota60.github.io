{
  "name": "react-parcel-starter",
  "version": "1.0.0",
  "description": "A setup to bootstrap my react apps fast",
  "author": "Szymon Wiszczuk",
  "license": "MIT",
  "dependencies": {
    "@types/react-router-dom": "^5.1.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-draggable": "^4.4.3",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "deploy": "parcel build src/index.html && gh-pages -d dist -b master",
    "purge": "rm -rf node_modules && rm -rf dist && rm -rf .parcel-cache"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.0.0-beta.2",
    "@parcel/transformer-typescript-tsc": "^2.0.0-alpha.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "gh-pages": "^2.2.0",
    "parcel": "^2.0.0-beta.2",
    "sass": "^1.26.2",
    "typescript": "^3.8.3"
  }
}
