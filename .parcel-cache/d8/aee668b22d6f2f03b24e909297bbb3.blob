.languageIcon {
  display: block;
  width: 1.2rem;
  height: 1.2rem;
  border-radius: 50%;
  background: linear-gradient(to right, #ee8d4c, yellow, violet);
  transform: rotate(45deg);
}
.languageIcon__HTML {
  background: #e34c26;
}
.languageIcon__CSS {
  background: #563d7c;
}
.languageIcon__SASS {
  background: #CF649A;
}
.languageIcon__Typescript {
  background: #2b7489;
}
.languageIcon__Javascript {
  background: #f1e05a;
}
.languageIcon__Node {
  background: #026E00;
}
.languageIcon__Express {
  background: #11ca39;
}
.languageIcon__React {
  background: #61DAFB;
}